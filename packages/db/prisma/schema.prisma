generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
enum tokenType {
  BTCUSDC
  SOLUSDC
  ETHUSDC
}
model User {
  id            String      @id @default(uuid())
  email         String      @unique
  lastLoggedIn  DateTime? 

}

model Asset {
  id            String      @id @default(uuid())
  symbol        tokenType
  imageUrl      String?
  name          String
  decimals      Int

}

model ExistingTrade {
  id                String      @id @default(uuid())
  openPrice         BigInt
  openPriceDecimal  Int          
}
